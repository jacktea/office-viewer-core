{
  "name": "onlyoffice-core",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "onlyoffice": {
    "version": "9.3.0.67",
    "submodule": "submodules/onlyoffice/web-apps",
    "sdkjsSubmodule": "submodules/onlyoffice/sdkjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build:onlyoffice": "tsx scripts/build-onlyoffice.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=24.12.0"
  }
}
